{
    "/api": {
      "target": "https://api.anthropic.com",
      "secure": true,
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": "/v1"
      },
      "headers": {
        "Content-Type": "application/json",
        "x-api-key": "YOUR_ANTHROPIC_API_KEY_HERE",
        "anthropic-version": "2023-06-01",
        "anthropic-dangerous-direct-browser-access": "true"
      }
    }
  }